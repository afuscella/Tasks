{
	"_version": "1.12.0",
	"sap.app": {
		"id": "br.com.tasks.Tasks",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "br.com.tasks.Tasks.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "br.com.tasks.Tasks.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}, {
				"uri": "css/header-style.css"
			}, {
				"uri": "css/bootstrap.min.css"
			}, {
				"uri": "css/search-style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "br.com.tasks.Tasks.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"bypassed": {
					"target": []
				},
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "Overview",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["Overview"]
			}],
			"targets": {
				"Overview": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Overview",
					"title": "{i18n>overviewTitle}",
					"viewId": "overview",
					"viewLevel": 1
				}
			}
		}
	}
}